{
    "presets": [
      "es2015", "react"
    ],
    "env": {
      "debug": {
        "sourceMaps": "inline",
        "retainLines": true
      }
    },
    "plugins": [
      ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ],
      ["module-resolver", {
        "root": ["./"]
      }]
    ]
  }
  
